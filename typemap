T_PERL_IO_HANDLE
	if ($arg && SvOK($arg) && SvROK($arg)) {
		$var = ($type)PerlIO_fileno( IoIFP( sv_2io(SvRV($arg)) ) );
	} else {
		$var = ($type)SvIV($arg);
	}
